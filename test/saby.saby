import std

function Fib = (number pos) => number {
    if pos <= 2 {
        return 1
    }
    else {
        return @(pos - 1) + @(pos - 2)
    }
}

function TestFunc = (float var1, function var2) => function {
    number var3 = var2((number)var1 * 10), var4 = 0
    while ++var4 != var3 {
        var3 += var4
    }
    return (string str) => string {
        return (string)var1 + " " + (string)var3
    }
}

print(Fib(10))

print(TestFunc(1.23, (number num) => number {
    if num < 10 {
        return num + 10
    }
    else {
        return @(num - 1) * @(num / 2)
    }
})("test test"))
