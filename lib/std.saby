var print = (string str) {
    asm {
        int  "AddString"
    }
}

var println = (string str) {
    asm {
        int  "AddString"
        mov  a1, '\n'
        int  "AddChar"
    }
}

var input = (string str) => string {
    asm {
        int  "AddString"
        news a1
        int  "GetString"
        mov  rv, a1
    }
}

export *